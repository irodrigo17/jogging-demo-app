source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'

def shared_pods
	pod 'JGProgressHUD', '~> 1.2'
	pod 'FormatterKit/URLRequestFormatter', '~> 1.7'
	pod 'ISO8601', '~> 0.1'
	pod 'XLForm', '~> 2.1'
end

target 'Jogging' do
	pod 'AFNetworking', '~> 2.0'
	shared_pods
end

target 'JoggingTests' do
	shared_pods
end

